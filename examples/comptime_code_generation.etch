fn main() -> void {
    let embedded_config: string = comptime(readFile("examples/data/config.txt"));
    print(embedded_config);

    comptime {
        let config_content: string = readFile("examples/data/config.txt");
        print(config_content);

        let debug_flag: int = 1;
        if debug_flag == 1 {
            print(999);
        }

        let i: int = 1;
        while i <= 3 {
            print(i);
            i = i + 1;
        }

        inject("injected_var", "string", config_content);
        inject("injected_num", "int", 42);
        inject("injected_i", "int", i);
    }

    print(42);
    print(injected_var);
    print(injected_num);
    print(injected_i);
}