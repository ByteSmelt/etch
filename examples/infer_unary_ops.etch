// Test unary operation type inference
fn main() {
    // Unary negation
    let neg_int = -42;         // int
    let neg_float = -3.14;     // float

    // Logical not
    let not_true = !true;      // bool
    let not_false = !false;    // bool

    print(neg_int);
    print(neg_float);
    print(not_true);
    print(not_false);
}